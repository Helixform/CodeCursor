{
    "name": "whalecloud-coding-vscode-plugin",
    "displayName": "浩鲸智能编程助手（WhaleCloud Coding Plus）",
    "description": "浩鲸智能编程助手（WhaleCloud Coding Plus） for Visual Studio Code",
    "version": "0.0.2",
    "publisher": "tigerwong",
    "repository": {
        "type": "git",
        "url": "https://github.com/wangxh89/whalecloud-coding-vscode-plugin"
    },
    "icon": "icon.png",
    "engines": {
        "vscode": "^1.74.0"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "whalecloud"
    ],
    "activationEvents": [
        "onCommand:*"
    ],
    "main": "./dist/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "whalecloud.generateCode",
                "title": "生成代码",
                "enablement": "activeEditor =~ /^workbench\\.editors\\./",
                "category": "WhaleCloud"
            },
            {
                "command": "whalecloud.addTests",
                "title": "添加测试（敬请期待）",
                "enablement": "activeEditor =~ /^workbench\\.editors\\./",
                "category": "WhaleCloud"
            },
            {
                "command": "whalecloud.whyBroken",
                "title": "为什么它坏了？（敬请期待）",
                "enablement": "activeEditor =~ /^workbench\\.editors\\./",
                "category": "WhaleCloud"
            },
            {
                "command": "whalecloud.explainPls",
                "title": "解释代码（敬请期待）",
                "enablement": "activeEditor =~ /^workbench\\.editors\\./",
                "category": "WhaleCloud"
            },
            {
                "command": "whalecloud.refactor",
                "title": "重构器（敬请期待）",
                "enablement": "activeEditor =~ /^workbench\\.editors\\./",
                "category": "WhaleCloud"
            },
            {
                "command": "whalecloud.showLastResult",
                "title": "Show Generated Code",
                "enablement": "whalecloud.hasActiveGenerateSession",
                "category": "WhaleCloud"
            },
            {
                "command": "whalecloud.acceptChanges",
                "title": "Accept Changes",
                "enablement": "activeEditor =~ /^workbench\\.editors\\./ && whalecloud.hasActiveGenerateSession",
                "category": "WhaleCloud"
            },
            {
                "command": "whalecloud.rejectChanges",
                "title": "Reject Changes",
                "enablement": "activeEditor =~ /^workbench\\.editors\\./ && whalecloud.hasActiveGenerateSession",
                "category": "WhaleCloud"
            },
            {
                "command": "whalecloud.resetChat",
                "title": "Reset Chat",
                "category": "WhaleCloud",
                "icon": "$(trash)"
            },
            {
                "command": "whalecloud.signInUp",
                "title": "Sign In / Sign Up",
                "category": "WhaleCloud"
            },
            {
                "command": "whalecloud.signOut",
                "title": "Sign Out",
                "category": "WhaleCloud"
            },
            {
                "command": "whalecloud.configureApiKey",
                "title": "Configure API Key",
                "category": "WhaleCloud"
            },
            {
                "command": "whalecloud.generateProject",
                "title": "Generate Project",
                "enablement": "workspaceFolders != null && workspaceFolderCount > 0",
                "category": "WhaleCloud"
            }
        ],
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "whalecloud",
                    "title": "WhaleCloud",
                    "icon": "activitybar-icon.svg"
                }
            ]
        },
        "views": {
            "whalecloud": [
                {
                    "type": "webview",
                    "id": "whalecloudchatview",
                    "name": "Chat"
                }
            ]
        },
        "menus": {
            "editor/context": [
                {
                    "submenu": "whalecloud.submenu"
                }
            ],
            "whalecloud.submenu": [
                {
                    "command": "whalecloud.generateCode"
                },
                {
                    "command": "whalecloud.addTests"
                },
                {
                    "command": "whalecloud.whyBroken"
                },
                {
                    "command": "whalecloud.explainPls"
                },
                {
                    "command": "whalecloud.refactor"
                }
            ],
            "view/title": [
                {
                    "command": "whalecloud.resetChat",
                    "when": "view == whalecloudchatview",
                    "group": "navigation"
                }
            ]
        },
        "submenus": [
            {
                "id": "whalecloud.submenu",
                "label": "WhaleCloud"
            }
        ],
        "configuration": {
            "title": "浩鲸智能编程助手",
            "properties": {
                "whalecloud.devCloudAccessToken": {
                    "type": "string",
                    "default": ""
                },
                "whalecloud.model": {
                    "type": "string",
                    "default": "gpt-3.5-turbo",
                    "enum": [
                        "gpt-3.5-turbo",
                        "gpt-4"
                    ],
                    "enumDescriptions": [
                        "Use GPT 3.5 Turbo model for code generation and chat",
                        "Use GPT 4 model for code generation and chat (requires access invitation)"
                    ],
                    "markdownDescription": ""
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run package",
        "compile": "webpack --mode production --devtool hidden-source-map",
        "compile:dev": "webpack",
        "compile-webview": "webpack -c ./webpack.webview.config.js --mode production --devtool hidden-source-map",
        "compile-webview:dev": "webpack -c ./webpack.webview.config.js",
        "compile-rust": "cd crates/cursor-core && wasm-pack build --target nodejs --weak-refs",
        "compile-rust:dev": "cd crates/cursor-core && wasm-pack build --target nodejs --weak-refs --dev",
        "copy-icons": "xcopy src\\webview\\codicons\\* dist\\ /s/e/y",
        "copy-rust": "xcopy crates\\cursor-core\\pkg\\cursor_core_bg.wasm dist\\cursor_core_bg.wasm /s/e/y",
        "copy-resources": "npm run copy-rust && npm run copy-icons",
        "watch": "webpack --watch",
        "package:no-rust": "npm run compile-webview:dev && npm run compile:dev && npm run copy-resources",
        "package:dev": "npm run compile-rust:dev && npm run package:no-rust",
        "package": "npm run compile-rust && npm run compile-webview && npm run compile && npm run copy-resources",
        "compile-tests": "tsc -p . --outDir out",
        "watch-tests": "tsc -p . -w --outDir out",
        "pretest": "npm run compile-tests && npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/diff-match-patch": "^1.0.32",
        "@types/glob": "^8.1.0",
        "@types/mocha": "^10.0.1",
        "@types/node": "16.x",
        "@types/react": "^18.0.28",
        "@types/react-dom": "^18.0.11",
        "@types/react-syntax-highlighter": "^15.5.6",
        "@types/vscode": "^1.74.0",
        "@types/vscode-webview": "^1.57.1",
        "@typescript-eslint/eslint-plugin": "^5.53.0",
        "@typescript-eslint/parser": "^5.53.0",
        "@vscode/test-electron": "^2.2.3",
        "css-loader": "^6.7.3",
        "eslint": "^8.34.0",
        "glob": "^8.1.0",
        "mocha": "^10.2.0",
        "style-loader": "^3.3.2",
        "ts-loader": "^9.4.2",
        "typescript": "^4.9.5",
        "webpack": "^5.75.0",
        "webpack-cli": "^5.0.1"
    },
    "dependencies": {
        "@vscode/codicons": "^0.0.32",
        "@vscode/webview-ui-toolkit": "^1.2.2",
        "diff-match-patch": "^1.0.5",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-markdown": "^8.0.6",
        "react-syntax-highlighter": "^15.5.0",
        "uuid": "^9.0.0"
    }
}
